{
    "name": "expressjs-starter-ts",
    "version": "0.0.1",
    "description": "Express JS Starter with TypeScript",
    "main": "./build/index.js",
    "scripts": {
        "dev": "ts-node-dev -r tsconfig-paths/register --respawn --rs ./src/app.ts",
        "build": "tsc",
        "postbuild": "tsx ./src/commands/SymbolicLink.ts",
        "start": "node ./build/app.js",
        "watch": "tsc -p tsconfig.json --watch",
        "migrate:create": "prisma migrate dev --create-only",
        "migrate": "prisma migrate dev",
        "migrate:prod": "prisma migrate deploy",
        "migrate:reset": "prisma migrate reset --force",
        "seed": "tsx ./prisma/seeders/DatabaseSeeder.ts",
        "prettier": "prettier --write ./**/*.ts",
        "prepare": "husky"
    },
    "keywords": [],
    "author": "Pramudya Wibowo",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "5.21.0",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "errsole": "^2.13.0",
        "errsole-postgres": "^2.3.0",
        "express": "^4.19.2",
        "express-validator": "^7.1.0",
        "helmet": "^7.1.0",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.30.1",
        "morgan": "^1.10.0",
        "multer": "1.4.5-lts.1",
        "node-cron": "^3.0.3",
        "prisma": "^5.21.0",
        "slug": "^9.1.0",
        "socket.io": "^4.8.1",
        "tsx": "^4.16.2",
        "typescript": "^5.5.4",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.12.0",
        "@types/bcrypt": "^5.0.2",
        "@types/body-parser": "^1.19.5",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/eslint__js": "^8.42.3",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.12",
        "@types/node": "^20.16.11",
        "@types/node-cron": "^3.0.11",
        "@types/slug": "^5.0.9",
        "@types/uuid": "^10.0.0",
        "eslint": "^9.12.0",
        "globals": "^15.10.0",
        "husky": "^9.1.6",
        "prettier": "3.2.5",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript-eslint": "^7.18.0"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@prisma/client",
            "@prisma/engines",
            "bcrypt",
            "esbuild",
            "prisma",
            "sqlite3"
        ]
    }
}
